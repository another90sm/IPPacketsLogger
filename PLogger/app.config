    <add name="DBConnectionString" providerName="MySQL" connectionString="Data source=D:\C#Projects\PLogger\PLogger\bin\Debug\db.sqlite" />
    <add name="DBConnectionString" providerName="MSSQL" connectionString="Data source=D:\C#Projects\PLogger\PLogger\bin\Debug\db.sqlite" />
    <add name="DBConnectionString" providerName="Oracle" connectionString="Data source=D:\C#Projects\PLogger\PLogger\bin\Debug\db.sqlite" />-->
  </connectionStrings>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2"/></startup></configuration>
